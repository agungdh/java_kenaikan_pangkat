	<queryString>
		<![CDATA[SELECT kp.*, p.nip, p.nama, pgl.pangkatgol lama, pgb.pangkatgol baru
FROM kenaikan_pangkat kp, pegawai p, pangkatgol pgl, pangkatgol pgb
WHERE kp.id_pegawai = p.id
AND kp.id_pangkat_lama = pgl.id
AND kp.id_pangkat_baru = pgb.id]]>
